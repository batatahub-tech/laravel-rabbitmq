{
    "name": "afonsoogomes/laravel-rabbitmq",
    "description": "Full RabbitMQ package for Laravel: manage exchanges, queues, bindings, publish and consume messages",
    "type": "library",
    "require": {
      "php": "^8.1",
      "illuminate/support": "^10.0|^11.0|^12.0",
      "illuminate/console": "^10.0|^11.0|^12.0",
      "php-amqplib/php-amqplib": "^3.0"
    },
    "autoload": {
      "psr-4": {
        "Vendor\\RabbitMQ\\": "src/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Vendor\\RabbitMQ\\Providers\\RabbitMQServiceProvider"
        ]
      }
    },
    "minimum-stability": "stable"
  }
  